<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/my_drawer"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryDark"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    tools:context=".HomeActivity">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >


        <ScrollView
            android:scrollbars="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.mohaa.dokan.Utils.searchbar.MaterialSearchBar

                    android:id="@+id/searchBar"
                    style="@style/MaterialSearchBarDark"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="8dp"
                    app:mt_navIconEnabled="true"
                    app:mt_placeholder="@string/what_are_u_looking_for" />
                <RelativeLayout

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/profile_background" >
                    <com.mohaa.dokan.Utils.shimmer.ShimmerFrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="190dp"
                        android:id="@+id/parentShimmerLayoutads"
                        android:duration="700">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="190dp"
                            android:orientation="horizontal">

                            <include layout="@layout/single_item_view_ads"/>


                        </LinearLayout>
                    </com.mohaa.dokan.Utils.shimmer.ShimmerFrameLayout>
                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/slider"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"

                        />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/background"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/background_mini" >
                    <com.mohaa.dokan.Utils.shimmer.ShimmerFrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/parentShimmerLayoutcategory"
                        android:duration="700">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:orientation="horizontal"
                            android:visibility="visible">

                            <include layout="@layout/layout_category_placeholder" />

                            <include layout="@layout/layout_category_placeholder" />

                            <include layout="@layout/layout_category_placeholder" />

                            <include layout="@layout/layout_category_placeholder" />


                        </LinearLayout>
                    </com.mohaa.dokan.Utils.shimmer.ShimmerFrameLayout>
                    <androidx.recyclerview.widget.RecyclerView

                        android:id="@+id/slider_cata"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"


                        android:layout_marginBottom="-1dp" />
                </RelativeLayout>

                <LinearLayout
                    android:gravity="center"
                    android:background="@color/colorPrimaryDark"
                    android:padding="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:weightSum="10">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:padding="5dp"
                        style="@style/headingfonts"
                        android:textColor="@color/white"
                        android:text="@string/we_are"
                        android:layout_weight="3"/>

                    <com.tomer.fadingtextview.FadingTextView
                        android:layout_width="wrap_content"
                        android:textColor="@color/white"
                        app:timeout="500"
                        style="@style/headingfonts"
                        android:gravity="center"
                        android:layout_height="match_parent"
                        app:texts="@array/examples"
                        android:layout_weight="7"/>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/recently_viewed_panel"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_weight="2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="16dp"
                        android:text="@string/just_for_you"
                        android:textColor="@color/colorAccent"
                        android:textStyle="bold"
                        android:textSize="20sp" />

                    <TextView

                        android:id="@+id/btn_recentrly"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="16dp"
                        android:background="@drawable/border_blue"
                        android:padding="8dp"
                        android:text="@string/shop_now"
                        android:textColor="@color/colorAccent"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.mohaa.dokan.Utils.shimmer.ShimmerFrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/parentShimmerLayout"
                        android:duration="700">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <include layout="@layout/single_item_view_mini"/>
                                <include layout="@layout/single_item_view_mini"/>
                                <include layout="@layout/single_item_view_mini"/>
                            </LinearLayout>

                        </LinearLayout>
                    </com.mohaa.dokan.Utils.shimmer.ShimmerFrameLayout>
                    <com.mohaa.dokan.Utils.multisnaprecyclerview.MultiSnapRecyclerView
                        android:id="@+id/first_recycler_view"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:orientation="horizontal"
                        app:msrv_gravity="start"
                        app:msrv_interval="1"
                        app:msrv_speed_ms_per_inch="100"/>
                </RelativeLayout>


                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    >


                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"

                        android:weightSum="10">

                        <androidx.cardview.widget.CardView

                            style="@style/homegridcard"
                            card_view:cardCornerRadius="@dimen/card_album_radius">

                            <RelativeLayout
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/border_blue"
                                android:id="@+id/laptop_panel"
                                >

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:background="@color/colorPrimaryDark"
                                    android:layout_width="match_parent"
                                    android:layout_height="150dp"
                                    android:scaleType="fitCenter"
                                    android:src="@drawable/logo"
                                    />
                                <TextView


                                    android:layout_height="30dp"
                                    android:layout_width="140dp"
                                    android:text="@string/up_to_50_off"
                                    android:textAlignment="center"
                                    android:textSize="18sp"
                                    android:textColor="#fff"
                                    android:background="@color/colorPrimaryDark"


                                    />
                                <LinearLayout
                                    android:id="@+id/store_information"
                                    android:layout_height="35dp"
                                    android:layout_width="match_parent"
                                    android:orientation="horizontal"
                                    android:background="@color/transparent_black"
                                    android:layout_alignParentBottom="true"
                                    >
                                    <TextView

                                        android:layout_height="match_parent"
                                        android:layout_width="0dp"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:text="@string/Loading"
                                        android:textSize="24sp"
                                        android:textStyle="bold"
                                        android:textColor="#fff"

                                        />


                                </LinearLayout>

                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            style="@style/homegridcard"
                            card_view:cardCornerRadius="@dimen/card_album_radius"
                            >

                            <RelativeLayout
                                android:id="@+id/acc_panel"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/border_blue">

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:background="@color/colorPrimaryDark"
                                    android:layout_width="match_parent"
                                    android:layout_height="150dp"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/logo"
                                    />
                                <TextView


                                    android:layout_height="30dp"
                                    android:layout_width="140dp"
                                    android:text="@string/up_to_70_off"
                                    android:textAlignment="center"
                                    android:textSize="18sp"
                                    android:textColor="#fff"
                                    android:background="@color/colorPrimaryDark"


                                    />
                                <LinearLayout

                                    android:layout_height="35dp"
                                    android:layout_width="match_parent"
                                    android:orientation="horizontal"
                                    android:background="@color/transparent_black"
                                    android:layout_alignParentBottom="true"
                                    >
                                    <TextView

                                        android:layout_height="match_parent"
                                        android:layout_width="0dp"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:text="@string/Loading"
                                        android:textSize="24sp"
                                        android:textStyle="bold"
                                        android:textColor="#fff"

                                        />


                                </LinearLayout>

                            </RelativeLayout>
                        </androidx.cardview.widget.CardView>

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_weight="2"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="16dp"
                            android:text="@string/best_products"
                            android:textColor="@color/colorAccent"
                            android:textStyle="bold"
                            android:textSize="20sp" />
                        <TextView
                            android:id="@+id/btn_suggest"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="16dp"
                            android:text="@string/shop_now"
                            android:textStyle="bold"
                            android:padding="8dp"
                            android:textColor="@color/colorAccent"
                            android:background="@drawable/border_blue"
                            android:textSize="16sp" />
                    </LinearLayout>


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <com.mohaa.dokan.Utils.shimmer.ShimmerFrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/parentShimmerLayout2"
                            android:duration="700">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:orientation="horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">
                                    <include layout="@layout/single_item_view_mini"/>
                                    <include layout="@layout/single_item_view_mini"/>
                                    <include layout="@layout/single_item_view_mini"/>
                                </LinearLayout>

                            </LinearLayout>
                        </com.mohaa.dokan.Utils.shimmer.ShimmerFrameLayout>
                        <com.mohaa.dokan.Utils.multisnaprecyclerview.MultiSnapRecyclerView

                            android:id="@+id/second_recycler_view"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:orientation="horizontal"
                            app:msrv_gravity="center"
                            app:msrv_interval="2"
                            app:msrv_speed_ms_per_inch="120" />

                    </RelativeLayout>



                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="10">

                        <androidx.cardview.widget.CardView
                            style="@style/homegridcard"
                            card_view:cardCornerRadius="@dimen/card_album_radius"
                            >

                            <RelativeLayout
                                android:id="@+id/fashion_him_panel"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/border_blue">

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:background="@color/colorPrimaryDark"
                                    android:layout_width="match_parent"
                                    android:layout_height="150dp"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/logo"
                                    />
                                <TextView


                                    android:layout_height="30dp"
                                    android:layout_width="140dp"
                                    android:text="@string/up_to_80_off"
                                    android:textAlignment="center"
                                    android:textSize="18sp"
                                    android:textColor="#fff"
                                    android:background="@color/colorPrimaryDark"


                                    />
                                <LinearLayout

                                    android:layout_height="35dp"
                                    android:layout_width="match_parent"
                                    android:orientation="horizontal"
                                    android:background="@color/transparent_black"
                                    android:layout_alignParentBottom="true"
                                    >
                                    <TextView

                                        android:layout_height="match_parent"
                                        android:layout_width="0dp"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:text="@string/Loading"
                                        android:textSize="18sp"
                                        android:textStyle="bold"
                                        android:textColor="#fff"

                                        />


                                </LinearLayout>

                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            style="@style/homegridcard"
                            card_view:cardCornerRadius="@dimen/card_album_radius"
                            >

                            <RelativeLayout
                                android:id="@+id/fashion_her_panel"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/border_blue">

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:background="@color/colorPrimaryDark"
                                    android:layout_width="match_parent"
                                    android:layout_height="150dp"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/logo"
                                    />
                                <TextView


                                    android:layout_height="30dp"
                                    android:layout_width="140dp"
                                    android:text="@string/up_to_80_off"
                                    android:textAlignment="center"
                                    android:textSize="18sp"
                                    android:textColor="#fff"
                                    android:background="@color/colorPrimaryDark"


                                    />
                                <LinearLayout

                                    android:layout_height="35dp"
                                    android:layout_width="match_parent"
                                    android:orientation="horizontal"
                                    android:background="@color/transparent_black"
                                    android:layout_alignParentBottom="true"
                                    >
                                    <TextView

                                        android:layout_height="match_parent"
                                        android:layout_width="0dp"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:text="@string/Loading"
                                        android:textSize="18sp"
                                        android:textStyle="bold"
                                        android:textColor="#fff"

                                        />


                                </LinearLayout>

                            </RelativeLayout>
                        </androidx.cardview.widget.CardView>

                    </LinearLayout>

                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_weight="2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="16dp"
                        android:text="@string/top_viewed"
                        android:textColor="@color/colorAccent"
                        android:textStyle="bold"
                        android:textSize="20sp" />
                    <TextView
                        android:id="@+id/btn_viewed"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="16dp"
                        android:text="@string/shop_now"
                        android:textStyle="bold"
                        android:padding="8dp"
                        android:textColor="@color/colorAccent"
                        android:background="@drawable/border_blue"
                        android:textSize="16sp" />
                </LinearLayout>


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.mohaa.dokan.Utils.shimmer.ShimmerFrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/parentShimmerLayout3"
                        android:duration="700">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <include layout="@layout/single_item_view_mini"/>
                                <include layout="@layout/single_item_view_mini"/>
                                <include layout="@layout/single_item_view_mini"/>
                            </LinearLayout>

                        </LinearLayout>
                    </com.mohaa.dokan.Utils.shimmer.ShimmerFrameLayout>
                    <com.mohaa.dokan.Utils.multisnaprecyclerview.MultiSnapRecyclerView
                        android:id="@+id/third_recycler_view"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:orientation="horizontal"
                        app:msrv_gravity="end"
                        app:msrv_interval="3"
                        app:msrv_speed_ms_per_inch="50" />
                </RelativeLayout>


                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    >






                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="10">

                        <androidx.cardview.widget.CardView
                            style="@style/homegridcard"
                            card_view:cardCornerRadius="@dimen/card_album_radius"
                            >

                            <RelativeLayout
                                android:id="@+id/tv_panel"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/border_blue">

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:background="@color/colorPrimaryDark"
                                    android:layout_width="match_parent"
                                    android:layout_height="150dp"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/logo"
                                    />
                                <TextView


                                    android:layout_height="30dp"
                                    android:layout_width="140dp"
                                    android:text="@string/up_to_70_off"
                                    android:textAlignment="center"
                                    android:textSize="18sp"
                                    android:textColor="#fff"
                                    android:background="@color/colorPrimaryDark"


                                    />
                                <LinearLayout

                                    android:layout_height="35dp"
                                    android:layout_width="match_parent"
                                    android:orientation="horizontal"
                                    android:background="@color/transparent_black"
                                    android:layout_alignParentBottom="true"
                                    >
                                    <TextView

                                        android:layout_height="match_parent"
                                        android:layout_width="0dp"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:text="@string/Loading"
                                        android:textSize="24sp"
                                        android:textStyle="bold"
                                        android:textColor="#fff"

                                        />


                                </LinearLayout>

                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>
                        <androidx.cardview.widget.CardView
                            style="@style/homegridcard"
                            card_view:cardCornerRadius="@dimen/card_album_radius"
                            >

                            <RelativeLayout
                                android:id="@+id/mobile_acc_panel"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/border_blue">


                                <androidx.appcompat.widget.AppCompatImageView
                                    android:background="@color/colorPrimaryDark"
                                    android:layout_width="match_parent"
                                    android:layout_height="150dp"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/logo"
                                    />
                                <TextView


                                    android:layout_height="30dp"
                                    android:layout_width="140dp"
                                    android:text="@string/up_to_80_off"
                                    android:textAlignment="center"
                                    android:textSize="18sp"
                                    android:textColor="#fff"
                                    android:background="@color/colorPrimaryDark"


                                    />
                                <LinearLayout

                                    android:layout_height="35dp"
                                    android:layout_width="match_parent"
                                    android:orientation="horizontal"
                                    android:background="@color/transparent_black"
                                    android:layout_alignParentBottom="true"
                                    >
                                    <TextView

                                        android:layout_height="match_parent"
                                        android:layout_width="0dp"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:text="@string/Loading"
                                        android:textSize="16sp"
                                        android:textStyle="bold"
                                        android:textColor="#fff"

                                        />


                                </LinearLayout>

                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                    </LinearLayout>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="10">

                        <androidx.cardview.widget.CardView
                            style="@style/homegridcard"
                            card_view:cardCornerRadius="@dimen/card_album_radius"
                            >

                            <RelativeLayout
                                android:id="@+id/camera_panel"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/border_blue">

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:background="@color/colorPrimaryDark"
                                    android:layout_width="match_parent"
                                    android:layout_height="150dp"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/logo"
                                    />
                                <TextView


                                    android:layout_height="30dp"
                                    android:layout_width="140dp"
                                    android:text="@string/up_to_50_off"
                                    android:textAlignment="center"
                                    android:textSize="18sp"
                                    android:textColor="#fff"
                                    android:background="@color/colorPrimaryDark"


                                    />
                                <LinearLayout

                                    android:layout_height="35dp"
                                    android:layout_width="match_parent"
                                    android:orientation="horizontal"
                                    android:background="@color/transparent_black"
                                    android:layout_alignParentBottom="true"
                                    >
                                    <TextView

                                        android:layout_height="match_parent"
                                        android:layout_width="0dp"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:text="@string/Loading"
                                        android:textSize="24sp"
                                        android:textStyle="bold"
                                        android:textColor="#fff"

                                        />


                                </LinearLayout>

                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>
                        <androidx.cardview.widget.CardView
                            style="@style/homegridcard"
                            card_view:cardCornerRadius="@dimen/card_album_radius"
                            >

                            <RelativeLayout

                                android:id="@+id/cleaning_panel"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/border_blue">

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:background="@color/colorPrimaryDark"
                                    android:layout_width="match_parent"
                                    android:layout_height="150dp"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/logo"
                                    />
                                <TextView


                                    android:layout_height="30dp"
                                    android:layout_width="140dp"
                                    android:text="@string/up_to_90_off"
                                    android:textAlignment="center"
                                    android:textSize="18sp"
                                    android:textColor="#fff"
                                    android:background="@color/colorPrimaryDark"


                                    />
                                <LinearLayout

                                    android:layout_height="35dp"
                                    android:layout_width="match_parent"
                                    android:orientation="horizontal"
                                    android:background="@color/transparent_black"
                                    android:layout_alignParentBottom="true"
                                    >
                                    <TextView

                                        android:layout_height="match_parent"
                                        android:layout_width="0dp"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:text="@string/Loading"
                                        android:textSize="24sp"
                                        android:textStyle="bold"
                                        android:textColor="#fff"

                                        />


                                </LinearLayout>

                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                    </LinearLayout>

                </LinearLayout>





            </LinearLayout>
        </ScrollView>

    </RelativeLayout>
</androidx.drawerlayout.widget.DrawerLayout>
